<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yc.user.mapper.MessageMapper">

    <insert id="insert">
        insert into message(uid,receiver,senddate,title,content)
        VALUE(#{uid},#{receiver},#{senddate},#{title},#{content})
    </insert>

</mapper>